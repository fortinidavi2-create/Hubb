prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/x86_64-linux-gnu
includedir=${prefix}/include/x86_64-linux-gnu

Name: libbsd-ctor
Description: Automatic constructor functions for libbsd
Version: 0.12.1
URL: https://libbsd.freedesktop.org/
Cflags: -I${includedir}
